{
  "private": true,
  "name": "@ever-dev/react-typescript-boilerplate",
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "start:prod": "webpack-dev-server --config webpack/prod.config.js",
    "build": "webpack --config webpack/prod.config.js",
    "format": "prettier --write './!(node_modules|dist)/**/*.{ts,tsx,js,jsx}'",
    "check": "prettier --check './!(node_modules|dist)/**/*.{ts,tsx,js,jsx}' && tsc --noEmit -p .",
    "test:e2e": "cypress run --headed --browser chrome",
    "test:e2e:watch": "cypress open",
    "test:e2e:ci": "start-server-and-test start:prod http-get://localhost:8080 test:e2e",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:ci": "jest --ci --passWithNoTests"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "mobx": "^5.15.4",
    "mobx-react-lite": "^1.5.2",
    "normalize.css": "^8.0.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react": "^6.24.1",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.1.0",
    "css-loader": "^3.4.2",
    "cypress": "^4.1.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.8",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  }
}
